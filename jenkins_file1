pipeline{
    agent any

    stages {
        stage('Hello'){
            steps{
                echo 'Hello World'
            }
        }
        stage('Build'){
            steps{
                echo 'Developpement'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Mise en production'
            }
        }
    }
}
